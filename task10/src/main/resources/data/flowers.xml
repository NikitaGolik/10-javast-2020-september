    <flower id="2" group="Cactus">
        <name>Carnegiea_gigantea</name>

        <soil>podzolic</soil>

        <origin>Mexico</origin>

        <visual_parametres>
            <stem_color>green</stem_color>
            <leaf_color>green</leaf_color>
            <average_plant_size>2000</average_plant_size>
        </visual_parametres>

        <growing_tips>
            <temperature>35</temperature>
            <light_requiring>true</light_requiring>
            <watering>200</watering>
        </growing_tips>

        <multiplying>seeds</multiplying>

    </flower>
</flowers>